import 'alphabet.grm' as a;

export dzienniki_nom = Optimize[StringFile['dzienniki-nom.tsv', 'utf8', 'utf8']];

dz_loc = CDRewrite[
    ("Dziennik ".utf8 : "Dzienniku ".utf8),
    "[BOS]",
    "",
    a.sigma_star];
nom_out = Project[dzienniki_nom, 'output'];
dz_nom_to_loc = Optimize[nom_out @ dz_loc];
export dzienniki_loc = Optimize[dzienniki_nom @ dz_nom_to_loc];
